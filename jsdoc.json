{
    "source":{ 
    "include": ["src"],
    "includepattern": ".js$",
    "excludepattern": "(node_modules/|docs)"
    },
    "plugins": ["plugins/markdown"],
    "templates": {
        "cleverLinks": true,
        "monospaceLinks": true
    },
    "opts": {
        "recurse": true,
        "description": "./docs/"
    }
}